{
	"name": "中国网络适应器",
	"version": "1.2",
	"description": "解决谷歌字体、谷歌JS库等资源在中国网络中受阻所导致的网页打开不畅问题",
	"permissions": [
			"webRequest",
			"webRequestBlocking",
			"storage",
			"alarms",
			"http://*/*",
			"https://*/*"
	],
	"background": {
		"scripts": [
			"data/js/configs.js",
			"background.js",
			"popup.js"
		]
	},
	"icons": {
		"32": "data/img/brunoect-cnnetadaptor.ico",
		"16": "data/img/16.png",
		"48": "data/img/48.png"
	},
	"browser_action": {
		"default_title": "中国网络适应器",
		"default_popup": "data/popup.html"
	},
	"options_page": "data/options.html",
	"author": "http://www.brunoxu.com",
	"homepage_url": "https://github.com/brunoxu/brunoect-cnnetadaptor-chrome",
	"update_url": "https://github.com/brunoxu/brunoect-cnnetadaptor-chrome/raw/master/updates.xml",
	"manifest_version": 2
}
